{
  "expo": {
    "name": "GharBudget",
    "slug": "GharBudget",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.jpg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundColor": "#ffffff",
        "backgroundColor": "#ffffff"
      },
      "package": "com.prashant180206.GharBudget"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/logo.jpg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/logo.jpg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "6e3c0be8-938e-40d1-a287-730b0bcc8752"
      }
    }
  }
}
